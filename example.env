# Basic API Settings
CACTICULTURES_APP_NAME=GPU-Text-Service
CACTICULTURES_DEBUG=true
CACTICULTURES_API_V1_STR=/api/v1
CACTICULTURES_ENVIRONMENT=development

# Ollama Settings
CACTICULTURES_OLLAMA_HOST=http://localhost:11434
CACTICULTURES_OLLAMA_MODEL=llama3.1:70b
CACTICULTURES_DEFAULT_TEMPERATURE=0.7
CACTICULTURES_MAX_TOKENS=131072
CACTICULTURES_MAX_CONTEXT_LENGTH=131072
CACTICULTURES_MODEL_EMBEDDING_SIZE=8192

# Model Specific Settings
CACTICULTURES_MODEL_BLOCK_COUNT=80
CACTICULTURES_MODEL_ATTENTION_HEAD_COUNT=64
CACTICULTURES_MODEL_ATTENTION_HEAD_COUNT_KV=8
CACTICULTURES_MODEL_FEED_FORWARD_LENGTH=28672

# CORS Settings (as JSON array)
CACTICULTURES_ALLOWED_ORIGINS='["https://dev.cacticultures.com", "https://staging.cacticultures.com", "https://cacticultures.com", "http://localhost:8000", "http://localhost:8001", "http://localhost:8002", "http://localhost:8003", "http://localhost:8004", "http://localhost:8005", "http://localhost:8006", "http://localhost:8007", "http://localhost:8008", "http://localhost:8009", "http://localhost:8010"]'

# Security Settings
CACTICULTURES_ACCESS_TOKEN_EXPIRE_MINUTES=30
CACTICULTURES_SECURITY_ALGORITHM=HS256
CACTICULTURES_RATE_LIMIT_PER_MINUTE=60

# Logging
CACTICULTURES_LOG_LEVEL=INFO
CACTICULTURES_LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"

# Model Response Configuration
CACTICULTURES_RESPONSE_CONFIG_TOP_P=0.9
CACTICULTURES_RESPONSE_CONFIG_TOP_K=50
CACTICULTURES_RESPONSE_CONFIG_REPEAT_PENALTY=1.1
CACTICULTURES_RESPONSE_CONFIG_PRESENCE_PENALTY=0
CACTICULTURES_RESPONSE_CONFIG_FREQUENCY_PENALTY=0

# Available Models
CACTICULTURES_AVAILABLE_MODELS='["llama3.1:70b", "qwq:32b"]'
